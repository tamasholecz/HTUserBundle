{
    "name": "tamasholecz/user-bundle",
    "description": "Symfony user bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Holecz Tam√°s",
            "email": "rasztamas79@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "symfony/form": "^4.0 || ^5.0",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "symfony/security-bundle": "^4.0 || ^5.0",
        "symfony/twig-bundle": "^4.0 || ^5.0",
        "symfony/validator": "^4.0 || ^5.0",
        "twig/twig": "^1.28 || ^2.0 || ^3.0",
        "symfony/templating": "^4.0 || ^5.0",
        "symfony/mailer": "^4.0 || ^5.0",
        "symfony/translation": "^4.0 || ^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3 || ^2.0",
        "doctrine/orm": "^2.6.3",
        "symfony/phpunit-bridge": "^4.0 || ^5.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "symfony/browser-kit": "^4.2|^5.0",
        "symfony/css-selector": "^4.2|^5.0",
        "phpunit/phpunit": "^4.8.35|^5.7.11|^6.5"
    },
    "autoload": {
        "psr-4": {
            "HT\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HT\\UserBundle\\Tests\\": "/Tests/"
        }
    }
}
